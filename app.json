{
  "expo": {
    "name": "signify",
    "slug": "signify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Signify-logo.png",
    "scheme": "signify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Signify-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdu911.signify",
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Signify-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
      "react-native-vision-camera",
      {
        "cameraPermissionText": "Signify needs access to your Camera."
      }
    ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "224391e5-233b-4502-a0af-90d996e90c57"
      }
    }
  }
}
